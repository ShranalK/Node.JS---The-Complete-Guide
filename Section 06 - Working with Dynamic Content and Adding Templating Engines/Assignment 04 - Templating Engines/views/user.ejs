<%- include('includes/head.ejs') %>
</head>
<body>
    <%- include('includes/navigation.ejs') %>
    <h1>Users</h1>
    <main>
        <% if (users.length > 0) { %>
            <ul>
                <% for (let user of users) { %>    
                    <li><%=user.name%></li>
                <% } %>
            </ul>
        <% } else { %>
            <h2>No users found</h2>
        <% } %>
    </main>   
</body>
</html>